{
  "name": "@dup4/starter-nestjs",
  "version": "0.0.0",
  "packageManager": "pnpm@7.9.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/Dup4/starter-nesjt#readme",
  "bugs": {
    "url": "https://github.com/Dup4/starter-nesjt/issues"
  },
  "license": "MIT",
  "author": "Dup4 <lyuzhi.pan@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dup4/starter-nesjt.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "nest start --watch",
    "repl": "nest start --entryFile repl",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest",
    "lint": "run-p lint:*",
    "lint:build": "tsc --noEmit",
    "lint:eslint": "eslint .",
    "lint:style": "prettier --check src/*.ts src/**/*.ts",
    "format": "run-p format:*",
    "format:eslint": "eslint --fix .",
    "format:style": "prettier --write src/*.ts src/**/*.ts",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.9",
    "@nestjs/core": "^9.0.9",
    "@nestjs/platform-express": "^9.0.9",
    "@types/object-path": "^0.11.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.5",
    "emoji-regex": "^10.1.0",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "git-repo-info": "^2.1.1",
    "js-yaml": "^4.1.0",
    "object-path": "^0.11.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@babel/types": "^7.18.10",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/testing": "^9.0.9",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "bumpp": "^7.2.0",
    "eslint": "^8.21.0",
    "esmo": "^0.14.1",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.9.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6",
    "vite": "^2.9.14",
    "vitest": "^0.21.1"
  }
}
